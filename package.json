{
  "name": "spigot-event-list",
  "description": "Bukkit, Spigot, Paper Event List",
  "keywords": [],
  "license": "ISC",
  "author": "sya_ri",
  "scripts": {
    "start:dev": "webpack serve --config webpack.config.dev.js",
    "start:product": "webpack serve --config webpack.config.product.js",
    "build": "webpack --config webpack.config.product.js",
    "download": "ts-node scripts/download-events.ts",
    "generate": "ts-node scripts/generate-output.ts",
    "lint": "eslint . --ext .json,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .json,.js,.ts,.jsx,.tsx --fix",
    "update": "yarn run download && yarn run generate",
    "ncu": "npx -p npm-check-updates -c \"ncu\"",
    "ncu:update": "npx -p npm-check-updates -c \"ncu -u\" && yarn",
    "sort-package-json": "npx sort-package-json"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "cheerio": "1.0.0-rc.10",
    "framer-motion": "^5",
    "handlebars": "4.7.7",
    "js-yaml": "4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "16.11.10",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/request-promise": "4.1.48",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "copy-webpack-plugin": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "html-minimizer-webpack-plugin": "^3.3.2",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
